pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'jenkins_slave', parameters: [string(name: 'NAME', value: 'Chinatip'), int(name: 'Age', value: 21)]
            }
        }
    }
}
