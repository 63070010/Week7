pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Chinatip', description: 'Enter your name')
        choice(name: 'AGE', defaultValue: '21', description: 'Choose your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.AGE}"
                
            }
        }
    }
}
